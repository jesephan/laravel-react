{
    "compilerOptions": {
        "jsx": "react-jsx",
        "module": "esnext",
        "target": "es6",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "typeRoots": ["./resources/js/types", "./node_modules/@types"]
    },
    "include": ["resources/js/**/*"]
}
